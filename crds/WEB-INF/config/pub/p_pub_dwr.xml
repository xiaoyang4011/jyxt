<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dwr PUBLIC "-//GetAhead Limited//DTD Direct Web Remoting 2.0//EN" 
    "http://getahead.org/dwr//dwr20.dtd">

<dwr>
	<!-- init is only needed if you are extending DWR -->
	<init>
	</init>

	<!-- without allow, DWR isn't allowed to do anything -->
	<allow>
		
	<!-- 机构、专业、班级三级联动实现  -->
		<create creator="spring" javascript="DepartThreeManage"> <!-- 这个名字是在页面里调用时用的 -->
			<param name="beanName" value="BODepartThreeManage" /> <!-- 这个是要被发布的类  -->
		</create>	
		

		<create creator="spring" javascript="SublibThreeLevel"> <!-- 这个名字是在页面里调用时用的 -->
			<param name="beanName" value="BOSublibThreeLevel" /> <!-- 这个是要被发布的类  -->
		</create>	

	</allow>
	
	

	<!-- you may need to tell DWR about method signatures -->
	<signatures></signatures>

</dwr>

